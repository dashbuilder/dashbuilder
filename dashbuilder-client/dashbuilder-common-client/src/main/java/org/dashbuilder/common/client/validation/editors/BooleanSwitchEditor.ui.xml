<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder' 
             xmlns:g='urn:import:com.google.gwt.user.client.ui'
             xmlns:b="urn:import:org.gwtbootstrap3.client.ui"
             xmlns:bts="urn:import:org.gwtbootstrap3.extras.toggleswitch.client.ui">
  <ui:style>
    .mainPanel {
      border: none;
    }
  </ui:style>

  <g:HTMLPanel ui:field="mainPanel" addStyleNames="{style.mainPanel} ">
    <b:Tooltip container="body" title="" ui:field="errorTooltip">
      <bts:ToggleSwitch ui:field="toggleSwitch" size="MINI" animate="true"/>
    </b:Tooltip>
  </g:HTMLPanel>
  
</ui:UiBinder>